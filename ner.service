[Unit]
Description=nervous-efficient-rebel (Stanford NER) service

[Service]
ExecStart=/home/ec2-user/ner/lib/index.js
Restart=always
User=nobody
Group=nobody
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/ec2-user/ner/lib/

[Install]
WantedBy=multi-user.target
